  stages {

    stage('Hello') {
      steps {
        sh '''
          java -version
        '''
      }
    }

    stage('cat README') {
      //when {
      //  branch "test*"
      //}

      steps {
        configFileProvider(
        [configFile(fileId: 'service_dockerfile', variable: 'DOCKERFILE_CONTENT')]){
          sh '''
          ls -a
          echo "ABC"
          cat README.md
          cat ${TEST}
          '''
        }
      }
    }
  }
}